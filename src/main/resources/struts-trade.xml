<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" 
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="admin_trade" namespace="/admin_trade" extends="admin-package">
	
		<action name="shop_*" class="com.imzhitu.admin.trade.ShopAction" method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
		<action name="shopType_*" class="com.imzhitu.admin.trade.ShopTypeAction" method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
	</package>
</struts>