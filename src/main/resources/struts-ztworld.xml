			method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
		<action name="zhituxiaomishu_*" class="com.imzhitu.admin.ztworld.ZHITUXiaoMiShuAction"
			method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
		<action name="child_*" class="com.imzhitu.admin.ztworld.ZTWorldChildAction"
			method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
		<action name="sticker_*" class="com.imzhitu.admin.ztworld.ZTWorldStickerAction"
			method="{1}">
			<result name="json" type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
	</package>

</struts>




